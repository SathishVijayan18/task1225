<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>ControllerClass</servlet-name>
  	<display-name>ControllerClass</display-name>
  	<description></description>
  	<servlet-class>com.controller.ControllerClass</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>PetController</servlet-name>
  	<display-name>PetController</display-name>
  	<description></description>
  	<servlet-class>PetController</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>ControllerClass</servlet-name>
  	<url-pattern>/ControllerClass</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>PetController</servlet-name>
  	<url-pattern>/PetController</url-pattern>
  </servlet-mapping>
</web-app>
